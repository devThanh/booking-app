<!--Booking start-->
<div class="container-fluid booking">
  <div class="container">
      <form    class="row g-3 mx-3 py-3 px-3 align-items-center justify-content-between bg-light shadow">
        <div class="col-auto">
          <label for="floatingSelect">Destination</label>
          <select onChange="myNewFunction(this);" class="form-select" id="floatingSelect"  aria-label="Floating label select example">
            <option selected>Chọn điểm đến</option>
            {{#each city}}
            <option value="{{this.slug}}">{{this.name}}</option>
            {{/each}}

          </select>
  
        </div>
        <div class="col-auto">
            <label for="endDate">Depart Date</label>
            <input id="endDate" class="form-control" type="date" />
            <span id="endDateSelected"></span>
        </div>
        <div class="col-auto">
            <label for="endDate">Return Date</label>
            <input id="endDate" class="form-control" type="date" />
            <span id="endDateSelected"></span>
        </div>
        <div class="col-auto">
          <label for="floatingSelect">Duration</label>
          <select class="form-select" id="floatingSelect" aria-label="Floating label select example">
            <option >Open this select menu</option>
            <option  value="1">One</option>
            <option value="2">Two</option>
            <option value="3">Three</option>
          </select>
        </div>
        <div class="col-auto">
          {{!-- <button class="btn btn-primary mt-4" style="width: 100px;">Submit</button> --}}
          <a  class="btn btn-primary mt-4 btn-submit" style="width: 100px;">Submit</a>
        </div>
      </form>
  </div>
</div>
<!--Booking end-->


